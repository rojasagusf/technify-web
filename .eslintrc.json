{
  "env": {
    "browser": true,
    "es2023": true
  },
  "extends": [
    "next/core-web-vitals",
    "eslint:all",
    "plugin:react/all"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": [
    "react"
  ],
  "plugins": [
    "react"
  ],
  "rules": {
    "array-element-newline": [
      "off"
    ],
    "arrow-body-style": [
      "off"
    ],
    "capitalized-comments": [
      "off"
    ],
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "dot-location": [
      "off"
    ],
    "function-call-argument-newline": [
      "error",
      "consistent"
    ],
    "indent": [
      "error",
      2
    ],
    "lines-between-class-members": [
      "off"
    ],
    "max-len": [
      "warn",
      {
        "code": 80
      }
    ],
    "max-lines-per-function": [
      "off"
    ],
    "max-statements": [
      "warn",
      20
    ],
    "new-cap": [
      "off"
    ],
    "no-console": [
      "off"
    ],
    "no-magic-numbers": [
      "off"
    ],
    "no-ternary": [
      "off"
    ],
    "no-underscore-dangle": [
      "off"
    ],
    "no-unused-vars": "off",
    "no-warning-comments": [
      "off"
    ],
    "one-var": [
      "off"
    ],
    "padded-blocks": [
      "error",
      "never"
    ],
    "quote-props": [
      "error",
      "as-needed"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".ts",
          ".tsx"
        ]
      }
    ],
    "react/jsx-indent": [
      "off"
    ],
    "react/jsx-indent-props": [
      "off"
    ],
    "react/jsx-max-depth": [
      "off"
    ],
    "react/react-in-jsx-scope": [
      "off"
    ],
    "react/jsx-max-props-per-line": [
      "off"
    ],
    "react/jsx-newline": [
      "off"
    ],
    "react/jsx-no-bind": [
      "error",
      {
        "allowArrowFunctions": true
      }
    ],
    "react/jsx-no-literals": [
      "off"
    ],
    "react/jsx-props-no-spreading": [
      "off"
    ],
    "react/jsx-sort-props": [
      "off"
    ],
    "react/require-default-props": [
      "off"
    ],
    "space-before-function-paren": [
      "error",
      "never"
    ]
  }
}
